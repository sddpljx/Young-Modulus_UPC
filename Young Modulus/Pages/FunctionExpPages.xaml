<UserControl x:Class="Young_Modulus.Pages.FunctionExpPages"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab SelectedSource="/Pages/FunctionExp/DataRW.xaml" Layout="List">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="数据读取和导出" Source="/Pages/FunctionExp/DataRW.xaml" />
                <mui:Link DisplayName="计算" Source="/Pages/FunctionExp/CalculateExp.xaml"/>
                <mui:Link DisplayName="数据列表" Source="/Pages/FunctionExp/DataGrid.xaml"/>
                <mui:Link DisplayName="滑动条" Source="/Pages/FunctionExp/Slider.xaml"/>
                <mui:Link DisplayName="上次计算结果" Source="/Pages/FunctionExp/LastCalExp.xaml"/>
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
